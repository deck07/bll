input[type="checkbox"] {
	position: relative;
	width: 1.618em;
	height: 1.618em;
	margin-top: .53em;
	margin-right: 1em;
	cursor: pointer;
	-webkit-transition: .15s;
	border-radius: 2em;
	background-color: #900;

	-webkit-appearance: none;
	-webkit-appearance: none;
